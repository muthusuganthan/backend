datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum role {
  SUPERADMIN
  ADMINn
}

model User {
  user_id   String @id @default(uuid()) // âœ… Fixed syntax
  user_name String
  email     String @unique
  password  String
  role      role
}
